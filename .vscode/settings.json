{
    "cmake.configureOnOpen": true,
    "cmake.installPrefix": "${userHome}/prefix/gnuradio",
    "cmake.configureSettings": {
        "ENABLE_DOXYGEN": "False",
    },
    "cmake.environment":{
        "PATH": "${userHome}/prefix/gnuradio/bin:${env.PATH}",
        "PYTHONPATH": "${userHome}/prefix/gnuradio/lib/python3.6/site-packages:${userHome}/prefix/gnuradio/lib/python3.6/dist-packages:${userHome}/prefix/gnuradio/lib/python3/site-packages:${userHome}/prefix/gnuradio/lib/python3/dist-packages:${env.PYTHONPATH}",
        "LD_LIBRARY_PATH": "${userHome}/prefix/gnuradio/lib:${env.LD_LIBRARY_PATH}",
        "LIBRARY_PATH": "${userHome}/prefix/gnuradio/lib:${env.LIBRARY_PATH}",
        "PKG_CONFIG_PATH": "${userHome}/prefix/gnuradio/lib/pkgconfig:${env.PKG_CONFIG_PATH}",
        "PYBOMBS_PREFIX": "${userHome}/prefix/gnuradio",
    },
    "terminal.integrated.env.linux": {
        "PATH": "${env:HOME}/prefix/gnuradio/bin:${env:PATH}",
        "PYTHONPATH": "${env:HOME}/prefix/gnuradio/lib/python3.6/site-packages:${env:HOME}/prefix/gnuradio/lib/python3/dist-packages:${env:PYTHONPATH}",
        "LD_LIBRARY_PATH": "${env:HOME}/prefix/gnuradio/lib:${env:LD_LIBRARY_PATH}",
        "LIBRARY_PATH": "${env:HOME}/prefix/gnuradio/lib:${env:LIBRARY_PATH}",
        "PKG_CONFIG_PATH": "${env:HOME}/prefix/gnuradio/lib/pkgconfig:${env:PKG_CONFIG_PATH}",
        "PYBOMBS_PREFIX": "${env:HOME}/prefix/gnuradio",
    },
    "files.associations": {
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "strstream": "cpp",
        "*.tcc": "cpp",
        "chrono": "cpp",
        "cstdint": "cpp",
        "deque": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "typeinfo": "cpp",
        "algorithm": "cpp",
        "iterator": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string": "cpp",
        "string_view": "cpp"
    },
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools"
}